{
    "name": "optimy/php-test-optimy",
    "description": "PHP Test Optimy",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Optimy\\PhpTestOptimy\\": "src/",
            "Optimy\\PhpTestOptimy\\Seeders\\": "seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Optimy\\PhpTestOptimy\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Philip Borbon",
            "email": "philipjohnborbon@gmail.com"
        }
    ],
    "require": {
        "php": "8.2.*",
        "symfony/dotenv": "^7.1",
        "ext-pdo": "*",
        "symfony/dependency-injection": "^7.1",
        "doctrine/orm": "3.2.2",
        "doctrine/migrations": "^3.8",
        "symfony/cache": "^7.1",
        "doctrine/data-fixtures": "^1.7",
        "symfony/process": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3"
    }
}
